{
  "name": "comviz",
  "description": "Open data analystics by the community",
  "version": "1.0.0",
  "private": true,
  "keywords": [
    "nextjs",
    "typescript"
  ],
  "scripts": {
    "dev": "next",
    "build": "next build",
    "start": "next start",
    "type-check": "tsc",
    "lint": "eslint --ignore-path .gitignore \"src/**/*.+(ts|js|tsx)\"",
    "format": "prettier --ignore-path .gitignore \"src/**/*.+(ts|js|tsx)\" --write",
    "postinstall": "husky install",
    "commit": "cz"
  },
  "lint-staged": {
    "./src/**/*.{ts,js,jsx,tsx}": [
      "yarn lint --fix",
      "yarn format"
    ]
  },
  "dependencies": {
    "@nivo/bar": "0.79.1",
    "@nivo/core": "0.79.0",
    "@nivo/pie": "0.79.1",
    "ace-builds": "1.4.14",
    "axios": "0.26.0",
    "cookies": "0.8.0",
    "daisyui": "2.2.2",
    "firebase": "9.6.7",
    "next": "12.1.0",
    "next-auth": "4.2.1",
    "react": "17.0.2",
    "react-ace": "9.5.0",
    "react-dom": "17.0.2"
  },
  "devDependencies": {
    "@commitlint/cli": "16.2.1",
    "@commitlint/config-conventional": "16.2.1",
    "@types/cookies": "0.7.7",
    "@types/node": "16.11.25",
    "@types/react": "17.0.39",
    "@types/react-dom": "17.0.11",
    "@typescript-eslint/eslint-plugin": "5.12.1",
    "@typescript-eslint/parser": "5.12.1",
    "autoprefixer": "10.4.2",
    "commitizen": "4.2.4",
    "cz-conventional-changelog": "3.3.0",
    "eslint": "8.9.0",
    "eslint-config-next": "12.1.0",
    "eslint-config-prettier": "8.4.0",
    "eslint-import-resolver-typescript": "2.5.0",
    "eslint-plugin-prettier": "4.0.0",
    "husky": "7.0.4",
    "lint-staged": "12.3.4",
    "postcss": "8.4.7",
    "prettier": "2.5.1",
    "tailwindcss": "3.0.23",
    "typescript": "4.5.5"
  },
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  }
}
